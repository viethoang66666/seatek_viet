#. module: sea_notifications
#: model:mail.template,body_html:sea_notifications.sea_notification_sale_order
msgid "\n"
"            <![CDATA[\n"
"            <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                <p>Dear khoa.huynh@seatek.vn,</p>\n"
"                <p>Here is notify for Sale Order - ${object.name}.</p>\n"
"                <ul>\n"
"                    <li>\n"
"                        <p>Customer: ${object.partner_id.name}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Salesperson: ${object.user_id.name}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Untaxed Amount: ${object.amount_untaxed}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Total: ${object.amount_total}</p>\n"
"                    </li>\n"
"                </ul>\n"
"                <p>Thank you,</p> \n"
"                <br/>\n"
"                <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                    <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                        <strong style=\"text-transform:uppercase;\">${object.user_id.company_id.name}</strong>\n"
"                    </h3>\n"
"                </div>\n"
"                <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                    <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                        ${object.user_id.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n"
"                    </span>\n"
"                    % if object.user_id.company_id.phone:\n"
"                        <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                            Phone:&nbsp; ${object.user_id.company_id.phone}\n"
"                        </div>\n"
"                    % endif\n"
"                    % if object.user_id.company_id.website:\n"
"                        <div>\n"
"                            Web :&nbsp;<a href=\"${object.company_id.website}\">${object.user_id.company_id.website}</a>\n"
"                        </div>\n"
"                    %endif\n"
"                    <p></p>\n"
"                </div>\n"
"            </div>\n"
"            \n"
"            "
msgstr "\n"
"            <![CDATA[\n"
"            <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                <p>Gửi khoa.huynh@seatek.vn,</p>\n"
"                <p>Đây là thông báo từ Đơn bán hàng - ${object.name}.</p>\n"
"                <ul>\n"
"                    <li>\n"
"                        <p>Khách hàng: ${object.partner_id.name}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Nhân viên kinh doanh: ${object.user_id.name}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Tổng tiền chưa thuế: ${object.amount_untaxed}</p>\n"
"                    </li>\n"
"                    <li>\n"
"                        <p>Tổng cộng: ${object.amount_total}</p>\n"
"                    </li>\n"
"                </ul>\n"
"                <p>Xin cảm ơn,</p> \n"
"                <br/>\n"
"                <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                    <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                        <strong style=\"text-transform:uppercase;\">${object.user_id.company_id.name}</strong>\n"
"                    </h3>\n"
"                </div>\n"
"                <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                    <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                        ${object.user_id.company_id.partner_id.sudo().with_context(show_address=True, html_format=True).name_get()[0][1] | safe}\n"
"                    </span>\n"
"                    % if object.user_id.company_id.phone:\n"
"                        <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                            Điện thoại:&nbsp; ${object.user_id.company_id.phone}\n"
"                        </div>\n"
"                    % endif\n"
"                    % if object.user_id.company_id.website:\n"
"                        <div>\n"
"                            Website :&nbsp;<a href=\"${object.company_id.website}\">${object.user_id.company_id.website}</a>\n"
"                        </div>\n"
"                    %endif\n"
"                    <p></p>\n"
"                </div>\n"
"            </div>\n"
"            \n"
"            "

